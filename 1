#include <stdio.h>

int sum(int a, int b);
int substract(int a, int b);
int divide(int a, int b);
int multiply(int a, int b);

int main(void) {
    int a, b;
    if (scanf("%d %d", &a, &b) != 2) {
        printf("Error: you need to enter numbers");
    }

    printf("%d %d %d ", sum(a, b), substract(a, b), multiply(a, b));
    if (b != 0) {
        printf("%d\n", divide(a, b));
    } else {
        printf("n/a\n");
    }
    return 0;
}

int substract(int a, int b) {
    return (a - b);
}

int multiply(int a, int b) {
    return (a * b);
}

int divide(int a, int b) {
    return (a / b);
}

int sum(int a, int b) {
  return (a + b);
}
